{
  "manifest_version": 3,
  "name": "ADvOps Quick Tools",
  "description": "A tool to complete repetative actions faster.",
  "version": "0.0.1",
  "permissions": ["storage", "tabs"],
  "content_scripts": [
    {
      "matches": ["http://*/*", "https://*/*"],
      "js": ["contentScript.js"]
    }
  ],
  "action": {
    "default_title": "ADvOps Quick Tools",
    "default_popup": "popup.html"
  }
}